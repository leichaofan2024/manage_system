<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Styles>
    <Style ss:ID="Description">
      <Alignment ss:Horizontal="Center" ss:Vertical="Center" ss:WrapText="1" ss:ShrinkToFit="1"/>
      <Borders>
      <Border ss:Position="Top" ss:Weight="1" ss:Color="#1c1c1c" ss:LineStyle="Continuous"/>
      <Border ss:Position="Bottom" ss:Weight="1" ss:Color="#1c1c1c" ss:LineStyle="Continuous"/>
      <Border ss:Position="Left" ss:Weight="1" ss:Color="#1c1c1c" ss:LineStyle="Continuous"/>
      <Border ss:Position="Right" ss:Weight="1" ss:Color="#1c1c1c" ss:LineStyle="Continuous"/>
      </Borders>
      <Interior ss:Pattern="Solid"/>
    </Style>
  </Styles>
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>

        <Cell ss:StyleID="Description"><Data ss:Type="String">序号</Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String">科室车间</Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String">综合考核排名奖励</Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String">发现和防止安全隐患奖励</Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String">奖励1</Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String">奖励2</Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String">奖励3</Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String">奖励4</Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String">奖励5</Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String">奖励6</Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String">备注</Data></Cell>

      </Row>
    <% @export_other_award_totals.each do |item| %>
      <Row>
        <Cell ss:StyleID="Description"><Data ss:Type="String"><%= item.序号 %></Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String"><%= item.科室车间 %></Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String"><%= item.排名奖励 %></Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String"><%= item.防止安全隐患奖励 %></Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String"><%= item.奖励1 %></Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String"><%= item.奖励2 %></Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String"><%= item.奖励3 %></Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String"><%= item.奖励4 %></Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String"><%= item.奖励5 %></Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String"><%= item.奖励6 %></Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String"><%= item.备注 %></Data></Cell>

      </Row>
    <% end %>
    </Table>
  </Worksheet>
</Workbook>
