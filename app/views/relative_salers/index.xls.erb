<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:o="urn:schemas-microsoft-com:office:office"
  xmlns:x="urn:schemas-microsoft-com:office:excel"
  xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
  xmlns:html="http://www.w3.org/TR/REC-html40">
  <Styles>
    <Style ss:ID="Description">
      <Alignment ss:Horizontal="Center" ss:Vertical="Center" ss:WrapText="1" ss:ShrinkToFit="1"/>
      <Borders>
      <Border ss:Position="Top" ss:Weight="1" ss:Color="#1c1c1c" ss:LineStyle="Continuous"/>
      <Border ss:Position="Bottom" ss:Weight="1" ss:Color="#1c1c1c" ss:LineStyle="Continuous"/>
      <Border ss:Position="Left" ss:Weight="1" ss:Color="#1c1c1c" ss:LineStyle="Continuous"/>
      <Border ss:Position="Right" ss:Weight="1" ss:Color="#1c1c1c" ss:LineStyle="Continuous"/>
      </Borders>
      <Interior ss:Pattern="Solid"/>
    </Style>
  </Styles>
  <Worksheet ss:Name="Sheet1">
    <Table>
      <Row>
        <Cell ss:StyleID="Description"><Data ss:Type="String">序号</Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String">科室车间</Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String">部门班组</Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String">工资号</Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String">姓名</Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String">系数</Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String">挂钩工资</Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String">安全质量</Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String">工作质量</Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String">一体化</Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String">兼职兼岗</Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String">其他</Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String">应发</Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String">考核扣款</Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String">合计</Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String">备注</Data></Cell>
      </Row>
    <% @export_relative_salers.each do |saler| %>
      <Row>
        <Cell ss:StyleID="Description"><Data ss:Type="String"><%= saler.序号 %></Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String"><%= saler.科室车间 %></Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String"><%= saler.部门班组 %></Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String"><%= saler.工资号 %></Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String"><%= saler.姓名 %></Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String"><%= saler.系数 %></Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String"><%= saler.挂钩工资 %></Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String"><%= saler.安全质量 %></Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String"><%= saler.工作质量 %></Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String"><%= saler.一体化 %></Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String"><%= saler.兼职兼岗 %></Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String"><%= saler.其他 %></Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String"><%= saler.应发 %></Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String"><%= saler.考核扣款 %></Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String"><%= saler.合计 %></Data></Cell>
        <Cell ss:StyleID="Description"><Data ss:Type="String"><%= saler.备注 %></Data></Cell>
      </Row>
    <% end %>
    </Table>
  </Worksheet>
</Workbook>
